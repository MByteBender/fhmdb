<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="331.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:id="mainPane" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.fhmdb.controller.WatchlistViewController">
   <HBox alignment="CENTER_LEFT" prefHeight="42.0" prefWidth="347.0" style="-fx-background-color: black;">
      <HBox alignment="TOP_LEFT" style="-fx-background-color: black" prefWidth="725.0" spacing="20.0">
         <padding>
            <Insets bottom="7.0" left="10.0" top="7.0"/>
         </padding>
         <Label style="-fx-background-color: F5C518;" text="FHMDb">
            <font>
               <Font name="Calibri Bold" size="24.0"/>
            </font>
            <padding>
               <Insets left="2.0" right="2.0"/>
            </padding>
         </Label>
         <Label styleClass="text-white" stylesheets="@styles.css" text="Welcome to FHMDb!"/>
      </HBox>
      <HBox alignment="TOP_RIGHT" style="-fx-background-color: black" prefWidth="100.0">
         <padding>
            <Insets bottom="7.0" left="10.0" top="7.0" right="10.0"/>
         </padding>
         <Button fx:id="changeListButton" styleClass="background-yellow" onAction="#loadHomeView" stylesheets="@styles.css" text="Home"/>
      </HBox>
   </HBox>
   <JFXListView fx:id="movieWatchlistView" stylesheets="@styles.css" VBox.vgrow="ALWAYS" />
</VBox>
